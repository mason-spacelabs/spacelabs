    <div class="loading-screen">
      <div id="loading-modal">
        <div class="text-center">
          <p class="loading-text"><i class="fas fa-spinner fa-pulse"></i><span> Retrieving Negotiated Rates</span>
          </p>
        </div>
      </div>
      <div id="checkout_options" class="modal-login-box hidden">
        <button class="atc-banner--close modal-close-box" type="button" aria-label="{{ 'general.accessibility.close' | t }}"  onclick="errorCart()">
        {%- include 'icon-menu-close' -%}
        </button>
        <section class="account-page-login">
          <header class="account-page-masthead modal-header-login">
            <img class="modal-image-login" src="{{ "spacelabs-logo.png" | asset_url }}">
          </header>
          <article class="account-page-content modal-content-login">
            <p class="rte">Pricing Error: We were unable to retrieve your negotiated rates. Please contact customer service at (425) 396-3300 for support.</p>
            <div class="form-action-row modal-close-button">
              <a href="/cart" style="width: 100%;">
                <button class="button-primary modal-button-login">
              Continue to Cart
                </button>
              </a>
            </div>
          </article>
        </section>
      </div>
      <div id="checkout_error" class="modal-login-box hidden">
        <button class="atc-banner--close modal-close-box" type="button" aria-label="{{ 'general.accessibility.close' | t }}"  onclick="errorCart()">
        {%- include 'icon-menu-close' -%} 
        </button>
        <section class="account-page-login">
          <header class="account-page-masthead modal-header-login">
            <img class="modal-image-login" src="{{ "spacelabs-logo.png" | asset_url }}">
          </header>
          <article class="account-page-content modal-content-login">
            <p class="rte">Pricing Error: We were unable to retrieve your negotiated rates. Please contact customer service at (425) 396-3300 for support.</p>
            <div class="form-action-row modal-close-button">
              
                <button class="button-primary modal-button-login" onclick="errorCheckout()">
              Continue to Checkout
                </button>
              
            </div>
          </article>
        </section>
      </div>
    </div>